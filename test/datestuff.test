import 'dart:test';
import 'package:step_on_it/database.dart';

Future main() async {
  test('Test Date.toString', (){
    Date d = Date(2028, 2, 29);
	expect(d.toString(), '2028-02-29');
  });

  test('Test Date.fromString', (){
    Date d = Date.fromString('2028-02-29');
	expect(d, Date(2028, 2, 29));
  });
}
